<Tab xmlns:fx="de.tum.imomesa" text="Attributes" closable="false" fx:controller="de.tum.imomesa.workbench.controllers.main.AttributeController">
	<graphic>
		<ImageView>
			<image>
				<Image url="icons/tabs/attribute.png"/>
			</image>
